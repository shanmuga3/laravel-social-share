{
    "name": "shanmuga/social-share",
    "description": "Share Social Media links with Laravel",
    "keywords": ["share", "social-share", "php", "laravel", "laravel7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shanmugarajan",
            "email": "shanmugarajan.33@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^5.5|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shanmuga\\SocialShare\\": "src/"
        }
    },
    "extra": {
